{
    "name": "color-names",
    "displayName": "color-names",
    "description": "",
    "version": "0.2.0",
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:color-names.generateColorNames"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "color-names.generateColorNames",
                "title": "Generate Color Names"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run package",
        "compile": "webpack --devtool nosources-source-map --config ./build/node-extension.webpack.config.js",
        "watch": "webpack --watch --devtool nosources-source-map --info-verbosity verbose --config ./build/node-extension.webpack.config.js",
        "package": "webpack --mode production --config ./build/node-extension.webpack.config.js",
        "test-compile": "tsc -p ./",
        "test-watch": "tsc -watch -p ./",
        "pretest": "yarn run test-compile && yarn run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "prerelease": "yarn run package",
        "release": "standard-version"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.51.0",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/parser": "^4.1.1",
        "eslint": "^7.9.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "standard-version": "^9.0.0",
        "ts-loader": "^8.0.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "axios": "^0.21.0",
        "closestvector": "^0.6.0"
    },
    "publisher": "eliaztray",
    "repository": {
        "url": "https://github.com/EliazTray/vscode-ext-color-names"
    }
}
